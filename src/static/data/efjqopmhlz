int main() 
{ 
    int i; 
    int arr[] = {10, 50, 30, 40, 20}; 
  
    int n = sizeof(arr) / sizeof(arr[0]); 
    int key = 30; 
  
    printf("Array before deletion\n"); 
    for (i = 0; i < n; i++) 
      printf("%d  ", arr[i]); 
  
    n = deleteElement(arr, n, key); 
  
    printf("\nArray after deletion\n"); 
    for (i = 0; i < n; i++) 
      printf("%d  ", arr[i]); 
  
    return 0; 
} 